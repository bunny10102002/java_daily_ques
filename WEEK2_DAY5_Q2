import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
public class Main
{
  public static void main(String[] args) 
  {
    Scanner obj= new Scanner(System.in);
    double x = obj.nextDouble();
    if (x >= 0) 
    {
      System.out.println((int)x);
    } 
      else
      {
      System.out.println("Invalid Input");
    }
  }
}
